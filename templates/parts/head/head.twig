<!doctype html>
<html>
    <head>
        <meta charset="{{ site.charset }}" />
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <title>
            {% if post.title is not defined %}
            Accueil - {{site.name}}
            {% else %} 
            {{post.title}} - {{site.name}}      
            {% endif %}       
        </title>

        <meta name="description" content="{{ site.description }}">
        <link rel="stylesheet" href="{{ site.theme.link }}/dist/styles/app.css" type="text/css" media="screen" />
        <script defer src="{{ site.theme.link }}/dist/scripts/app.js"></script>

        <meta name="twitter:card" content="avatar de caeiinos" >
        <meta name="twitter:site" content="@JeanDeRoy1" >
        <meta name="twitter:creator" content="@JeanDeRoy1" >
        <meta name="twitter:title" content="Jean De Roy" >
        <meta name="twitter:description" content="{{ site.description }}" >
            
        <meta name="twitter:image" content="{{ site.url }}/dist/assets/images/logo.png" >

        <meta property ="og:site__name" content="jean-deroy" />
        <meta property ="og:title" content="Jean De Roy" />
        <meta property ="og:description" content="{{ site.description }}" />
        <meta property ="og:type" content="website" />
        <meta property ="og:url" content="{{ site.url }}" />
        <meta property ="og:image" content="{{ site.url }}/dist/assets/images/logo.png" />
        <meta property ="og:image:width" content="1200">
        <meta property ="og:image:height" content="630">


        <link rel="author" href="{{ site.theme.link }}/humans.txt" />
        <link rel="pingback" href="{{ site.pingback_url }}" />
        <link rel="profile" href="http://gmpg.org/xfn/11">
        {{function('wp_head')}}